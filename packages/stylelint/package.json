{
  "name": "@usa/stylelint",
  "version": "0.1.0",
  "repository": "https://github.com/cardo-usa/usa.git",
  "license": "MIT",
  "private": true,
  "main": "index.cjs",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint './**/{*.json,*.*js,*.*ts}'",
    "lint:fix": "eslint './**/{*.json,*.*js,*.*ts}' --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write ."
  },
  "devDependencies": {
    "@usa/prettier": "workspace:*",
    "eslint-config-usa-base": "workspace:*",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.6.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-prettier": "4.1.0"
  }
}
